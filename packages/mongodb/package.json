{
    "name": "@axiomkit/mongodb",
    "version": "0.0.4",
    "type": "module",
    "description": "Axiomkit Mongodb Memory",
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        "./package.json": "./package.json",
        ".": {
